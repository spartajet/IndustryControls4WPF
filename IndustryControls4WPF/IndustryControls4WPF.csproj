<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1;net452;net462;net472</TargetFrameworks>
    
    <UseWPF>true</UseWPF>
    <Version>3.0.1</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/spartajet/IndustryControls4WPF</RepositoryUrl>
    <PackageProjectUrl>https://github.com/spartajet/IndustryControls4WPF</PackageProjectUrl>
    <RepositoryType>github</RepositoryType>
    <Description>A TTL signal setting and display control for WPF.</Description>
    <Copyright>Copyright © 2001-2020 Xiaozhong Guo, All Rights Reserved</Copyright>
    <Authors>Dr. Xiaozhong Guo</Authors>

