<Window x:Class="IndustryControls4WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IndustryControls4WPF"
        xmlns:ttl="clr-namespace:IndustryControls4WPF.Controls.Digital"
        mc:Ignorable="d"
        Title="MainWindow" Height="260" Width="660">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>
        <ttl:TtlConfigurator x:Name="TtlConfigurator" Margin="10"></ttl:TtlConfigurator>
        <Button Content="删除" Grid.Row="1" Click="ButtonBase_OnClick"></Button>
    </Grid>
</Window>
